---
import Container from "../components/Container.astro";
import Layout from "../layouts/Layout.astro";
import Team from "../components/Team.astro";
import Contact from "../components/Contact.astro";
---

<Layout title="Fathomage Software">
    <main class="space-y-20">
    <Container>
        div class="py-8 px-4 mx-auto max-w-screen-xl lg:py-16 lg:px-6">
        <div class="mx-auto max-w-screen-sm text-center lg:mb-16 mb-8">
            <h2 class="mb-4 text-3xl lg:text-4xl tracking-tight font-extrabold text-gray-900">Connect with Us</h2>
            <p class="font-light text-gray-500 sm:text-xl">Let's Discuss Your Software Solution</p>
        </div>  
    </div>
    <Contact />
  </main>
</Layout>
